{
  "name": "next-pos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "unit:test": "./node_modules/.bin/nyc --reporter=lcov --reporter=text ./node_modules/.bin/mocha --recursive -r ts-node/register -r tsconfig-paths/register './tests/**/*.spec.ts'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "chai": "^4.3.7",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "next": "13.3.2",
    "next-auth": "^4.22.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.5",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/next-auth": "^3.15.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/configs/*.ts",
      "**/data/*.ts",
      "**/*.d.ts",
      "**/enums/**/*.ts",
      "**/models/**/*.ts",
      "**/entities/**/*.ts",
      "**/factories/**/*.ts",
      "**/repositories/**/*.ts",
      "**/lib/secrets/*.ts",
      "**/lib/db/*.ts",
      "**/lib/utils/*.ts",
      "**/lib/request/*.ts",
      "**/lib/schemas/*.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90
  }
}
